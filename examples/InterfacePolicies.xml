<!-- dn=uni -->
<polUni>
    <infraInfra>
        <!-- Interface Link Speed Policies for 10G, 25G RS-FEC, 40G and 100G -->
        <fabricHIfPol annotation="" autoNeg="on" descr="" dfeDelayMs="0" dn="uni/infra/hintfpol-10G" fecMode="inherit" linkDebounce="100" name="10G" nameAlias="" ownerKey="" ownerTag="" speed="10G" userdom=":all:"/>
        <fabricHIfPol annotation="" autoNeg="on" descr="" dfeDelayMs="0" dn="uni/infra/hintfpol-25G-RS-FEC" fecMode="cl91-rs-fec" linkDebounce="100" name="25G-RS-FEC" nameAlias="" ownerKey="" ownerTag="" speed="25G" userdom=":all:"/>
        <fabricHIfPol annotation="" autoNeg="on" descr="" dfeDelayMs="0" dn="uni/infra/hintfpol-40G" fecMode="inherit" linkDebounce="100" name="40G" nameAlias="" ownerKey="" ownerTag="" speed="40G" userdom=":all:"/>
        <fabricHIfPol annotation="" autoNeg="on" descr="" dfeDelayMs="0" dn="uni/infra/hintfpol-100G" fecMode="inherit" linkDebounce="100" name="100G" nameAlias="" ownerKey="" ownerTag="" speed="100G" userdom=":all:"/>
        <!-- LLDP Neighbor Policies to enable or disable -->
        <lldpIfPol adminRxSt="disabled" adminTxSt="disabled" annotation="" descr="" dn="uni/infra/lldpIfP-Disable-LLDP" name="Disable-LLDP" nameAlias="" ownerKey="" ownerTag="" userdom=":all:"/>
        <lldpIfPol adminRxSt="enabled" adminTxSt="enabled" annotation="" descr="" dn="uni/infra/lldpIfP-Enable-LLDP" name="Enable-LLDP" nameAlias="" ownerKey="" ownerTag="" userdom=":all:"/>
        <!-- CDP Neighbor Policies to enable or disable -->
        <cdpIfPol adminSt="disabled" annotation="" descr="" dn="uni/infra/cdpIfP-Disable-CDP" name="Disable-CDP" nameAlias="" ownerKey="" ownerTag="" userdom=":all:"/>
        <cdpIfPol adminSt="enabled" annotation="" descr="" dn="uni/infra/cdpIfP-Enable-CDP" name="Enable-CDP" nameAlias="" ownerKey="" ownerTag="" userdom=":all:"/>
        <!-- LACP Mode Active Policy -->
        <lacpLagPol annotation="" ctrl="fast-sel-hot-stdby,graceful-conv,susp-individual" descr="" dn="uni/infra/lacplagp-LACP-Mode-Active" maxLinks="16" minLinks="1" mode="off" name="LACP-Mode-Active" nameAlias="" ownerKey="" ownerTag="" userdom=":all:"/>
        <!-- LACP Mode Active Policy with Suspend on Individual Disabled -->
        <lacpLagPol annotation="" ctrl="fast-sel-hot-stdby,graceful-conv" descr="" dn="uni/infra/lacplagp-LACP-Mode-Active-No-SOI" maxLinks="16" minLinks="1" mode="off" name="LACP-Mode-Active-No-SOI" nameAlias="" ownerKey="" ownerTag="" userdom=":all:"/>
        <!-- LACP Mode On Policy -->
        <lacpLagPol annotation="" ctrl="fast-sel-hot-stdby,graceful-conv,susp-individual" descr="" dn="uni/infra/lacplagp-LACP-Mode-On" maxLinks="16" minLinks="1" mode="off" name="LACP-Mode-On" nameAlias="" ownerKey="" ownerTag="" userdom=":all:"/>
    </infraInfra>
</polUni>